<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle" >
            <solid android:color="@color/white" />
            <size android:width="100dp" android:height="100dp"/>
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="15" android:toDegrees="45" android:pivotX="0" android:pivotY="0">
            <shape android:shape="rectangle">
                <solid android:color="@color/greenMint"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="-15" android:toDegrees="0" android:pivotX="100" android:pivotY="0">
            <shape android:shape="rectangle">
                <solid android:color="@color/greenMint"/>
            </shape>
        </rotate>
    </item>
</layer-list>